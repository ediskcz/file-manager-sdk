{
    "name": "edisk/file-manager-sdk",
    "description": "File Manager PHP SDK",
    "keywords": [
        "file manager",
        "php",
        "sdk"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Edisk\\FileManager\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Edisk\\FileManager\\Tests\\": "tests/src/"
        },
        "classmap": [
            "tests/src/"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ediskcz/common.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "cocur/slugify": "^4.2",
        "doctrine/orm": "^2.17",
        "doctrine/doctrine-bundle": "^2.10",
        "edisk/common": "dev-php8.0",
        "geoip2/geoip2": "^2.12",
        "league/flysystem": "^3.0",
        "nelexa/zip": "^4.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.5"
    }
}
