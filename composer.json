{
    "name": "sovic/file-manager-sdk",
    "description": "File Manager PHP SDK",
    "keywords": [
        "file manager",
        "php",
        "sdk"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "FileManager\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "php": "^8.0",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "geoip2/geoip2": "^2.12",
        "cocur/slugify": "^4.2",
        "nelexa/zip": "^4.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.5"
    }
}
